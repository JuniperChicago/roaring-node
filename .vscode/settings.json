{
  "clang-format.language.javascript.enable": false,
  "clang-format.language.typescript.enable": false,
  "clang-format.language.c.enable": true,
  "clang-format.language.cpp.enable": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "jest.autoEnable": false,
  "prettier.eslintIntegration": false,
  "prettier.parser": "babylon",
  "prettier.requireConfig": true,
  "tslint.autoFixOnSave": true,
  "tslint.exclude": "node_modules/**/*",
  "tslint.ignoreDefinitionFiles": false,
  "files.associations": {
    ".envrc": "shellscript",
    "*.idl": "avro",
    "roaring.c": "cpp",
    "__functional_03": "cpp",
    "functional": "cpp",
    "__tree": "cpp",
    "__bit_reference": "cpp",
    "__split_buffer": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "iterator": "cpp",
    "map": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "vector": "cpp",
    "__config": "cpp",
    "__nullptr": "cpp",
    "__locale": "cpp",
    "__functional_base": "cpp",
    "__functional_base_03": "cpp",
    "__tuple": "cpp",
    "chrono": "cpp",
    "limits": "cpp",
    "memory": "cpp",
    "ratio": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "system_error": "cpp",
    "atomic": "cpp",
    "bitset": "cpp",
    "locale": "cpp",
    "queue": "cpp",
    "ios": "cpp",
    "__string": "cpp",
    "__mutex_base": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "streambuf": "cpp",
    "cstddef": "cpp",
    "exception": "cpp",
    "stdexcept": "cpp",
    "typeinfo": "cpp",
    "algorithm": "cpp",
    "cmath": "cpp",
    "hash_map": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "hashtable": "cpp",
    "__hash_table": "cpp"
  },
  "python.linting.enabled": false
}
