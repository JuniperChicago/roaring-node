{
  "name": "roaring",
  "version": "1.0.5",
  "description": "CRoaring official port for NodeJS",
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "gypfile": true,
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild -j 12",
    "rebuild": "prebuild --compile",
    "compile": "node-gyp build -j 12",
    "recompile": "node-gyp rebuild -j 12",
    "test": "jest",
    "tslint": "node ./node_modules/tslint/lib/tslintCli.js --project tsconfig.json --exclude **/node_modules/** --exclude **/test/**",
    "eslint": "node ./node_modules/eslint/bin/eslint.js .",
    "doc": "node ./node_modules/.bin/ts-docs-gen --config ./docs-gen.json",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js",
    "benchmarks/": "node --expose-gc ./scripts/benchmarks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "files": [
    "src/cpp/**/*.*",
    "src/cpuinfo/**/*.*",
    "lib/*.js",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "CRoaring",
    "Roaring",
    "bitmaps"
  ],
  "author": "Salvatore Previti",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "dependencies": {
    "prebuild-install": "^5.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "eslint-config-quick": "^0.1.23",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "fastbitset": "^0.2.8",
    "jest": "^24.8.0",
    "physical-cpu-count": "^2.0.0",
    "prebuild": "^9.0.0",
    "prettier": "^1.18.2",
    "ts-docs-gen": "^0.2.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-quick": "^0.1.17",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  }
}
