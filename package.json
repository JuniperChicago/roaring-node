{
  "name": "roaring",
  "version": "0.2.8",
  "description": "CRoaring port for NodeJS",
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "gypfile": true,
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild -j 12",
    "rebuild": "prebuild --compile",
    "compile": "node-gyp build -j 12",
    "recompile": "node-gyp rebuild -j 12",
    "test": "jest",
    "tslint": "node ./node_modules/tslint/lib/tslint-cli.js --project tsconfig.json --exclude **/node_modules/** --exclude **/test/**",
    "eslint": "node ./node_modules/eslint/bin/eslint.js .",
    "doc": "node ./node_modules/.bin/ts-docs-gen --config ./docs-gen.json",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js",
    "benchmarks/": "node --expose-gc ./scripts/benchmarks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "files": [
    "src/cpp/**/*.*",
    "src/cpuinfo/**/*.*",
    "lib/*.js",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "CRoaring"
  ],
  "author": "Salvatore Previti",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "dependencies": {
    "prebuild-install": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "fastbitset": "^0.2.7",
    "jest": "^23.1.0",
    "physical-cpu-count": "^2.0.0",
    "prebuild": "^7.6.0",
    "prettier": "^1.13.5",
    "ts-docs-gen": "^0.2.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.2"
  }
}
